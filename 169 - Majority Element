class Solution {
    public int majorityElement(int[] nums) {
         int cur = -1;
        int count = 0;
        for (int i = 0; i < nums.length; i++) {
            if(count==0){
                cur = nums[i];
                count++;
            }
            else{
            if(cur==nums[i]){
                count++;
            }else{
                count--;
            }
            }
        }
        return cur;
    }       
}
