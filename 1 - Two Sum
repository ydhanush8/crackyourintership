class Solution {
    public int[] twoSum(int[] nums, int target) {
        HashMap<Integer,Integer> map = new HashMap<>();
        for(int i=0;i<nums.length;i++){
            int num = nums[i];
            Integer rem = (Integer)target - num;
            if(map.containsKey(rem)){
                return new int[]{map.get(rem) , i};
            }
            map.put(nums[i],i);
        }
        return new int[]{-1,-1};
    }
}
